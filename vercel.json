{
  "version": 2,
  "builds": [
    {
      "src": "vercel-build.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [{ "handle": "vercel-build", "src": "/_vercel_build_status" }],
  "functions": { "vercel-build": { "runtime": "nodejs14.x" } },
  "env": {
    "PRISMA_TELEMETRY_INFORMATION": "off"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "postbuild": {
      "prisma": "generate"
    }
  }
}
